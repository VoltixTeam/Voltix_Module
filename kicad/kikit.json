{
    "layout": {
        "type": "grid",
        "rows": 2,
        "cols": 4,
        "hspace": "2mm",
        "vspace": "2mm",
        "rotation": "180deg",
        "alternation": "rows",
        "renamenet": "Board_{n}-{orig}",
        "renameref": "{orig}"
    },
    "source": {
        "type": "auto",
        "tolerance": "50mm"
    },
    "tabs": {
        "type": "annotation"
    },
    "cuts": {
        "type": "vcuts",
        "offset": "-0.2mm",
	"clearance": "0.6mm"
    },
    "framing": {
        "type": "frame",
        "width": "15mm"
    },
    "post": {
        "type": "auto",
        "millradius": "1mm",
        "refillzones": true
    },
    "copperfill": {
        "type": "hatched",
	"width": "0.5mm",
	"spacing": "1mm",
	"orientation": "0deg",
	"layers": "F.Cu,B.Cu",
	"clearance": "1mm"
    },
    "fiducials": {
        "type": "3fid",
        "hoffset": "5mm",
        "voffset": "5mm",
        "coppersize": "2mm",
        "opening": "2.5mm"
    },
    "tooling": {
        "type": "4hole",
        "hoffset": "10mm",
        "voffset": "5mm",
        "size": "2mm"
    }
}
